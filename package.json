{
  "name": "worker",
  "version": "1.0.0",
  "description": "Worker for dealing with runpod",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -p tsconfig.json --watch\" \"nodemon dist/index.js\"",
    "lint": "eslint . -c src/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "author": "Max Carlson",
  "license": "Apache 2",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.925.0",
    "@aws-sdk/s3-request-presigner": "^3.925.0",
    "@bull-board/api": "^6.7.10",
    "@bull-board/express": "^6.7.10",
    "@types/multer": "^2.0.0",
    "bullmq": "^5.30.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "express": "^4.21.2",
    "express-basic-auth": "^1.2.1",
    "form-data": "^4.0.4",
    "multer": "^2.0.2",
    "runpod-sdk": "^1.0.7",
    "ts-toolbelt": "^9.6.0",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.10.6",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.13.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.3.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.20.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
